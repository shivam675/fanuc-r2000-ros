<?xml version="1.0"?>
<robot name="base_bot"  xmlns:xacro="http://ros.org/wiki/xacro">
    
    
    <link name="world"/>


<joint name="world_joint" type="fixed">
    <parent link="world" />
    <child link = "base_blob" />
    <origin xyz="1.5 0 0" rpy="0.0 0.0 0" />
</joint>

    
    
    
    
    
    <link name="base_blob">
      <inertial>
        <mass value="1.584994"/>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <inertia
          ixx="0.01042" ixy="0.001177" ixz="-0.0008871"
          iyy="0.01045" iyz="0.0002226"
          izz="0.01817"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="package://fanuc_gazebo/mesh/base_table.stl" scale="1 1 1"/>
        </geometry>
        <material name="RED">
          <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://fanuc_gazebo/mesh/base_table.stl" scale="1 1 1" />
        </geometry>
      </collision>
</link>


<joint name="main_joint" type="revolute">
      <parent link="base_link" />
      <child link = "link_1" />
      <origin xyz="0 0 0.62" rpy="0.0 0.0 0.0" />
      <axis xyz="0 0 1" />
      <limit lower="-3.142" upper="3.142" effort="150.0" velocity="1.5"/>
</joint>




    <link name="rot_blob">
      <inertial>
        <mass value="1.584994"/>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
        <inertia
          ixx="0.01042" ixy="0.001177" ixz="-0.0008871"
          iyy="0.01045" iyz="0.0002226"
          izz="0.01817"/>
      </inertial>
      <visual>
        <geometry>
          <mesh filename="package://fanuc_gazebo/mesh/rot_block.stl" scale="1 1 1"/>
        </geometry>
        <material name="RED">
          <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://fanuc_gazebo/mesh/rot_block.stl" scale="1 1 1" />
        </geometry>
      </collision>
</link>

</robot>